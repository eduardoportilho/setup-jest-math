{
  "name": "setup-jest-math",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Eduardo <eduardo@portilho.com> (http://www.portilho.com/)",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "jest": "^20.0.4"
  },
  "scripts": {
    "test": "jest --coverage",
    "watch": "jest --watch"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "collectCoverageFrom": [
      "*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 50,
        "branches": 50,
        "functions": 50,
        "lines": 100
      }
    }
  }
}
